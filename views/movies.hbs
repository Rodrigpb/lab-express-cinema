
<div class="container">
  <div class="row">
    {{#each this}}
    <div class="col-md-4">
      <div class="card">
        <img class="card-img-top" src="{{this.image}}" alt="{{this.title}} "/>
        <div class="card-body">
          <h5 class="card-title">
            {{this.title}}
          </h5>
          <a href="/movie/{{movie._id}}">See more</a>
        </div>
      </div>
    </div>
    {{/each}}
  </div>
</div>
